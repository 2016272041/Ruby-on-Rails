<h1> List of All Articles </h1>

<table>
    <tr>
        <th>Title</th>
        <th>Author</th>
        <th>Text</th>
        <th>Time-created</th>
    </tr>

    <% @articles.each do |article| %>
    <tr>
        <td><%= article.title %></td>
        <td><%= article.author %></td>
        <td><%= article.text %></td>
        <td><%= article.created_at %></td>
    </tr>
    <% end %>
</table>